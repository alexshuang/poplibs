{
    "tests": [
        {
            "name": "single conv layer - Sim",
            "program": "${ROOT_DIR}/poplibs/tools/single_conv_layer_random.py",
            "args": [
                "--seed", "${TEST_SEED}",
                "--device-type", "Sim",
                "--binary", "${BUILD_DIR}/build/poplibs/tools/single_conv_layer"
            ],
            "testArgs": {
                "timeout": 1200,
                "skip": [77, 245],
                "bailout": 10
            },
            "weight": 9,
            "parallel": 40
        },
        {
            "name": "multi conv layer - Sim",
            "program": "${ROOT_DIR}/poplibs/tools/single_conv_layer_random.py",
            "args": [
                "--seed", "${TEST_SEED}",
                "--device-type", "Sim",
                "--binary", "${BUILD_DIR}/build/poplibs/tools/multi_conv_layer",
                "--large",
                "--json",
                "--num-convs", "0"
            ],
            "testArgs": {
                "timeout": 1200,
                "skip": [77, 245],
                "bailout": 10
            },
            "weight": 1,
            "parallel": 40
        }
    ]
}
